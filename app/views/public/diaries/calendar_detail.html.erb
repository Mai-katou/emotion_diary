<h1><%= @start_date %>の日記一覧</h1>

<% @diaries.each do |diary| %>

<p>感情</p>
<% diary.image_emotions.each do |emotion| %>
  <%= emotion.emotion.emotion_name %>
<% end %>

<p>日記</p>
<%= diary.diary_detail %>

<p>画像</p>
<% diary.images.each do |image| %>
<%= image_tag image, size: "100x100" %>
<% end %>

<%= link_to '編集', edit_diary_path(diary.id) %>
<%= link_to '削除', diary_path(diary.id), method: :delete %>

<% end <%></%>